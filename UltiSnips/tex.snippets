snippet ref "Reference"
\ref{$1} $0
endsnippet

snippet multicol "MultiColumns"
\begin{multicols}{${2:$1}}
  ${VISUAL} $0
\end{multicols}
endsnippet

snippet begin "Begin"
\begin{$1}
  ${VISUAL} $0
\end{$1}
endsnippet

snippet eq "Equation"
\begin{equation}\label{eq:$1}
  ${VISUAL} $0
\end{equation}
endsnippet
